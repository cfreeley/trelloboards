{
	"name": "__MSG_extensionName__",
	"version": "1.0",
	"manifest_version": 2,
	"description": "__MSG_extensionName__",
	"icons": {
		"128": "icon-128.png",
		"32": "icon-32.png",
		"48": "icon-48.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["https://api.trello.com/"],
	"content_security_policy": "script-src 'self' https://api.trello.com https://ssl.google-analytics.com; object-src 'self'",
	"default_locale": "en"
}